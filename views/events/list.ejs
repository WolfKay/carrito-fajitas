<ul class="list-group col-xs-12">
    <% events.forEach((event) => { %>
        <li class="list-group-item list-selectable" id="<%= event.id%>">
            <div class="container-fluid">
                <div class="row-fluid">
                    <div class="col-xs-9 col-sm-10 inside-list">
                        <div><%= event.name %></div>
                        <div><%= moment(event.eventDate).format("MMM Do YYYY") %></div>
                    </div>
                    <div class="col-xs-3 col-sm-2 inside-list">
                        <div><img class="small-icon" src="/visual-assets/dish-icon.png"/> <%= event.numberPeople %></div>
                        <div><%= event.price %> €</div>
                    </div>
                </div>
                <div class="row-fluid list-hidable list-hidden">
                    <div class="col-xs-9 col-sm-10 inside-list">
                        <div><%= event.name %></div>
                        <div><%= moment(event.eventDate).format("MMM Do YYYY") %></div>
                    </div>
                    <div class="col-xs-3 col-sm-2 inside-list">
                        <div><img class="small-icon" src="/visual-assets/dish-icon.png"/> <%= event.numberPeople %></div>
                        <div><%= event.price %> €</div>
                    </div>
                </div>
            </div>                    
        </li>
    <% }) %>
</ul>