{
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "es6": true
    },
  "installedESLint": true,
  "parserOptions": {
  "ecmaFeatures": {
    "experimentalObjectRestSpread": true,
      "jsx": true
  },
    "sourceType": "module"
	},
  "plugins": [
    "ejs"
  ],
  "rules": {
    "linebreak-style": [
      "error",
      "unix"
    ],
    "semi": [
      1,
      "always"
    ]
  }
}
